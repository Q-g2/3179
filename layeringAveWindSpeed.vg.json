{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": " average Wind speed data across continent in the background.",
    "data": {"url": "https://raw.githubusercontent.com/Q-g2/3179/main/ModifiedGlobalWeatherRepository.csv"},
    "transform": [{"filter": "datum.visibility_miles==6"}],
    "layer": [{
      "mark": {"type": "point", "opacity": 0.3},
      "encoding": {
        "x": {"field":"Continent", "type": "nominal"},
        "y": {"field": "wind_mph", "type": "quantitative"}
      }
    }, {
      "mark": "line",
      "encoding": {
        "x": {"field":"Continent", "type": "nominal"},
        "y": {"aggregate": "mean", "field": "wind_mph"}
      }
    }]
  }
  